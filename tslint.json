{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "no-empty": false,
    "no-console": false,
    "curly": [true, "ignore-same-line"],
    "typedef": [true, "call-signature"]
  },
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts"]
  }
}
